{
  "name": "ethibox",
  "homepage": "https://ethibox.fr",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ston3o/ethibox.git"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "TOKEN=$(cat $TELEPRESENCE_ROOT/var/run/secrets/kubernetes.io/serviceaccount/token) NODE_ENV=production node public/index.js",
    "start:dev": "npm-run-all --parallel watch:server watch:client",
    "watch:client": "webpack --watch -d",
    "watch:server": "nodemon -i test -i public -i src/client --exec TOKEN=$(cat $TELEPRESENCE_ROOT/var/run/secrets/kubernetes.io/serviceaccount/token) babel-node -- src/server/index.js",
    "build": "NODE_ENV=production babel --minified --no-comments --compact true -d public/ src/server && webpack -p --progress --colors",
    "test": "cypress run",
    "test:ci": "start-server-and-test start:prod http://localhost:4444 test"
  },
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
